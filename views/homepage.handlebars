<div class="container-fluid row border border-success my-container m-0 p-1">
  <!-- container 1 -->
  {{> public-navbar}}

        <!-- container 2 -->
      <div class="col-lg-8 text-center col-sm-12">
        <!-- carousel 1 -->
        <h4>Top Games</h4>
        <div
          id="carouselExampleCaptions"
          class="carousel slide"
          data-bs-ride="carousel"
        >
          <div class="carousel-inner">
            <div class="carousel-item active">
              <div class="d-flex justify-content-center">
                <img
                  src="http:{{topResults.1.cover.url}}"
                  class="d-block w-50"
                  alt="..."
                />
              </div>
              <div class="carousel-caption d-none d-md-block">
                <h5>{{topResults.1.name}}</h5>
                <p>
                  Some representative placeholder content for the first slide.
                </p>
              </div>
            </div>
            {{#each topResults as |topResult|}}
            <div class="carousel-item">
              <div class="d-flex justify-content-center">
                <img
                  src="http:{{cover.url}}"
                  class="d-block w-50"
                  alt="..."
                />
              </div>
              <div class="carousel-caption d-none d-md-block">
                <h5>{{name}}</h5>
                <p>
                  Some representative placeholder content for the first slide.
                </p>
              </div>
            </div>
            {{/each}}
          </div>
          <button
            class="carousel-control-prev"
            type="button"
            data-bs-target="#carouselExampleCaptions"
            data-bs-slide="prev"
          >
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
          </button>
          <button
            class="carousel-control-next"
            type="button"
            data-bs-target="#carouselExampleCaptions"
            data-bs-slide="next"
          >
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
          </button>
        </div>
        <!-- carousel 2 -->
        <h4>player games</h4>
        <div
          id="carouselExampleCaptions2"
          class="carousel slide"
          data-bs-ride="carousel"
        >
          <div class="carousel-indicators">
            <button
              type="button"
              data-bs-target="#carouselExampleCaptions2"
              data-bs-slide-to="0"
              class="active"
              aria-current="true"
              aria-label="Slide 1"
            ></button>
            <button
              type="button"
              data-bs-target="#carouselExampleCaptions2"
              data-bs-slide-to="1"
              aria-label="Slide 2"
            ></button>
            <button
              type="button"
              data-bs-target="#carouselExampleCaptions2"
              data-bs-slide-to="2"
              aria-label="Slide 3"
            ></button>
          </div>
          <div class="carousel-inner">
            <div class="carousel-item active">
              <div class="d-flex justify-content-center">
                <img
                  src="../assets/img/341CCFC6-09BB-4267-8556-D000BF219458.JPG"
                  class="d-block w-50"
                  alt="..."
                />
              </div>
              <div class="carousel-caption d-none d-md-block">
                <h5>First slide label</h5>
                <p>
                  Some representative placeholder content for the first slide.
                </p>
              </div>
            </div>
            <div class="carousel-item">
              <div class="d-flex justify-content-center">
                <img
                  src="../assets/img/46321097-885E-44BA-9976-819DCD38DA1C.JPG"
                  class="d-block w-50"
                  alt="..."
                />
              </div>
              <div class="carousel-caption d-none d-md-block">
                <h5>Second slide label</h5>
                <p>
                  Some representative placeholder content for the second slide.
                </p>
              </div>
            </div>
            <div class="carousel-item">
              <div class="d-flex justify-content-center">
                <img
                  src="../assets/img/60F115F4-C2F7-41C7-A3CB-B8BD2B51A975.JPG"
                  class="d-block w-50"
                  alt="..."
                />
              </div>
              <div class="carousel-caption d-none d-md-block">
                <h5>Third slide label</h5>
                <p>
                  Some representative placeholder content for the third slide.
                </p>
              </div>
            </div>
          </div>
          <button
            class="carousel-control-prev"
            type="button"
            data-bs-target="#carouselExampleCaptions2"
            data-bs-slide="prev"
          >
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
          </button>
          <button
            class="carousel-control-next"
            type="button"
            data-bs-target="#carouselExampleCaptions2"
            data-bs-slide="next"
          >
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
          </button>
        </div>
      </div>

      <!-- container 3 -->
      <div class="col-lg-2 col-sm-12">
        <h4>Friends List</h4>
        <div class="list-group">
          <button
            type="button"
            class="list-group-item list-group-item-action active text-center"
            aria-current="true"
          >
            friend 1
          </button>
          <button
            type="button"
            class="
              list-group-item list-group-item-action
              text-center text-center
            "
          >
            friend 2
          </button>
          <button
            type="button"
            class="list-group-item list-group-item-action text-center"
          >
            friend 3
          </button>
          <button
            type="button"
            class="list-group-item list-group-item-action text-center"
          >
            friend 4
          </button>
          <button
            type="button"
            class="list-group-item list-group-item-action text-center"
          >
            friend 5
          </button>
        </div>
      </div>
    </div>